{
  "name": "t3ac/t3up_bootstrap",
  "type": "typo3-cms-extension",
  "description": "T3UP - Bootstrap plugin",
  "authors": [
    {
      "name": "Michael Lang",
      "role": "Developer",
      "email": "michael.lang@h-da.de"
    }
  ],
  "license": ["GPL-2.0-or-later"],
  "keywords": [
    "TYPO3",
    "extension",
    "t3up",
    "gridelements"
  ],
  "repositories": [
    {
	    "type": "vcs",
	    "url" : "https://github.com/t3ac/t3up_bootstrap.git"
    }
  ],
  "require": {
	  "t3ac/t3up_bootstrap": "*"
  },
  "autoload": {
    "psr-4": {
      "T3up\\T3upBootstrap\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "t3up_bootstrap"
    }
  }
}